	<script type="text/javascript">
	$(function() {
	$( 'ul.nav li' ).on( 'click', function() {
		    $( this ).parent().find( 'li.active' ).removeClass( 'active' );
		    $( this ).addClass( 'active' );
			 
		});
	});
//	$(function() {

	//});
	function onload(){
		// document.aboutCookie = "about-active1";	
	//	var x = document.aboutCookie;
		//alert(x);
			/*  $(document).on('click', '#navbar li', function(){
				$('.navbar-toggle').click();
	});  */
			 $(document).on('click', '#navbar li', function(){
				 $('.navbar-toggle').trigger( "click" );
		}); 
			 
		var currenturl=window.location.href;
		if(currenturl=="http://localhost:3004/DMANTZ_Home_Page#about")
			{
			$('.about-us').addClass('active');
		//	   var current = document.getElementsByClassName("active");
		  //   current[0].className = current[0].className.replace(" active", "");
		    // className="about";
		    // this.className = "active";
			}
		else if(currenturl=="http://localhost:3004/DMANTZ_Home_Page#contact")
			{
			$('.contact-us').addClass('active');
			}
		else if(currenturl=="http://localhost:3004/DMANTZ_Home_Page#")
		{
		$('.home-us').addClass('active');
		}
	}
	</script>
